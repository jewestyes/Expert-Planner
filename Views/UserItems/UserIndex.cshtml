@{
	Layout = "_Layout";
}

<div class="window-container">
	<h2>User List</h2>

<table class="table">
    <thead>
        <tr>
            <th>Имя</th>
            <th>Фамилия</th>
            <th>Отчество</th>
            <th>Должность</th>
            <th>Город</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.MiddleName</td>
                <td>@user.LastName</td>
                <td>@user.Position</td>
                <td>@user.City</td>
            </tr>
        }
    </tbody>
</table>
</div>
